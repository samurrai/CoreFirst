@{
    ViewData["Title"] = "Home Page";
    var items = ViewBag.Items;
}

    <div class="text-center">
        <h1 class="display-4">Список продуктов</h1>
        @foreach (var item in items)
        {
            <div>
                <h3>@item.Name</h3>
                <p>@item.Price</p>
                <p>@item.Amount</p>
            </div>
            <br />
        }
    </div>
